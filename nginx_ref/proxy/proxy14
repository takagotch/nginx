http{
  server{
    proxy_buffering       on;
    proxy_buffer-size     8k;
    proxy_buffers number  4k;

    proxy_buffering       on;
    proxy_buffer_size     32k;
    proxy_buffers 100     32k;

    proxy_buffering       off;

    proxy_temp_path           /var/cache/nginx/proxy_temp;
    proxy_max_temp_file_size  4096m;
  }
}
